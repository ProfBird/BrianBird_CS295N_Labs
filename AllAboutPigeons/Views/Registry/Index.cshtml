@model Message
@{
    ViewData["Title"] = "Pigeon Registry";
}


<h1>Pigeon Registry</h1>
<a href="ForumPost">Enter a new post</a>

<h2>Forum Post</h2>
@if (@Model != null)
{
    <p>

        To: @Model.To.Name
        <br />
        From: @Model.From.Name
        <br />
        Message: @Model.Text
        <br />
        Date: @Model.Date
        <br>
        Rating: @Model.Rating
<br />
    </p>
}
